{
  "name": "auth-app-infrastructure",
  "version": "1.0.0",
  "description": "CDK Infrastructure for Authentication App",
  "main": "dist/lib/index.js",
  "scripts": {
    "build": "tsc",
    "build-lambdas": "./build-lambdas.sh",
    "build-all": "npm run build-lambdas && npm run build",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build-all && cdk deploy",
    "destroy": "npm run build && cdk destroy",
    "diff": "npm run build && cdk diff",
    "synth": "npm run build-all && cdk synth",
    "clean": "rm -rf dist/ cdk.out/"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2",
    "aws-cdk": "2.103.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.103.1",
    "constructs": "^10.0.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "lambda",
    "api-gateway",
    "dynamodb",
    "authentication"
  ],
  "author": "",
  "license": "MIT"
}